{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps && npm list terser || npm install terser --save-dev",
  "rewrites": [
    {
      "source": "/institutions",
      "destination": "/index.html"
    },
    {
      "source": "/institutions/",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

