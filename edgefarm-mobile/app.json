{
  "expo": {
    "name": "XFUEL Mobile",
    "slug": "xfuel-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "xfuel",
    "userInterfaceStyle": "dark",
    "platforms": ["ios", "android"],
    "newArchEnabled": true,
    "description": "XFUEL Protocol mobile: Swap TFUEL to LSTs, track balances, and join Early Believers.",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#05050a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.xfuel.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#05050a"
      },
      "package": "app.xfuel.mobile",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "xfuel.app",
              "pathPrefix": "/"
            },
            {
              "scheme": "xfuel"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "routerAddress": "0xYourRealRouterWillGoHere_AfterDeploy",
      "thetaMainnetRpc": "https://eth-rpc-api.thetatoken.org/rpc",
      "thetaMainnetChainId": 361,
      "thetaExplorerUrl": "https://explorer.thetatoken.org",
      "apiUrl": "https://api.xfuel.app",
      "eas": {
        "projectId": "4d7c789a-9ec5-482c-b119-34ce6dad1bab"
      }
    },
    "plugins": [
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4d7c789a-9ec5-482c-b119-34ce6dad1bab"
    }
  }
}
