{
  "expo": {
    "name": "EdgeFarm",
    "slug": "edgefarm-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "edgefarm",
    "userInterfaceStyle": "dark",
