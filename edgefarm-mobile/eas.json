{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_NETWORK": "testnet"
      }
    },
    "mainnet": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_NETWORK": "mainnet"
      },
      "channel": "mainnet-beta"
    }
  }
}
