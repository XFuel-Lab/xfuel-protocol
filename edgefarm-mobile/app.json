    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.edgefarm.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#05050a"
      },
      "package": "com.edgefarm.mobile",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "lotteryJackpot": "$50k",
      "globalLotteryCutBps": 1000,
      "routerAddress": "0xYourRealRouterWillGoHere_AfterDeploy",
      "eas": {
        "projectId": "4d7c789a-9ec5-482c-b119-34ce6dad1bab"
      }
    },
    "plugins": [
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4d7c789a-9ec5-482c-b119-34ce6dad1bab"
    }
  }
